{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUncheckedIndexedAccess": true,
    "exactOptionalPropertyTypes": true,
    "noUncheckedSideEffect": true
  },
  "include": [
    "src/app/**/new-*",
    "src/components/new-*",
    "src/lib/new-*"
  ]
}
