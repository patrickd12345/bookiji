{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../dist/voice-console/assets",
    "rootDir": "./src",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "target": "ES2022",
    "lib": ["ES2022", "DOM"],
    "noEmit": false,
    "declaration": false,
    "skipLibCheck": true,
    "types": ["node"]
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
