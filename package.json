{
  "name": "bookiji",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --cache --cache-location .eslintcache",
    "lint:crit": "eslint . --max-warnings=0 --rule \"no-unused-vars: off\" --rule \"@typescript-eslint/no-explicit-any: off\" --rule \"react/no-unescaped-entities: off\" --ignore-pattern \"packages/**/*\" --ignore-pattern \"tests/setup.ts\"",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "context": "node startup-context.js",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:e2e": "playwright test tests --reporter=html",
    "crawl": "node scripts/crawl-site.mjs",
    "generate:e2e": "node scripts/generate-tests.mjs",
    "qa:pipeline": "node scripts/qa-pipeline.mjs run",
    "qa:crawl": "node scripts/qa-pipeline.mjs crawl",
    "qa:generate": "node scripts/qa-pipeline.mjs generate",
    "qa:test": "node scripts/qa-pipeline.mjs test",
    "qa:report": "node scripts/qa-pipeline.mjs report",
    "qa:help": "node scripts/qa-pipeline.mjs help",
    "test:pw": "cd packages/pw-tests && playwright test --reporter=list",
    "test:a11y": "playwright test tests/a11y --project=chromium",
    "test:a11y-dialog": "cd packages/pw-tests && playwright test tests/a11y --grep '@a11y-dialog' --reporter=list",
    "test:perf": "playwright test tests/perf --project=chromium",
    "test:smoke": "cd packages/pw-tests && playwright test tests --grep '@smoke' --reporter=list",
    "perf:lhci": "lhci autorun --config=lighthouserc.json || true",
    "quality:index": "node scripts/quality-index.mjs",
    "quality:dashboard": "node scripts/quality-index.mjs && echo 'Dashboard: ./quality-dashboard/index.html'",
    "ci": "pnpm lint:crit && pnpm typecheck && pnpm test:unit && pnpm test:e2e",
    "format": "prettier --write .",
    "codemod:unused": "node scripts/codemods/unused-vars.mjs",
    "codemod:any": "node scripts/codemods/any-to-unknown.mjs",
    "codemod:auto": "pnpm codemod:unused && pnpm codemod:any",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:ci": "vitest run --reporter=dot --coverage",
    "test:coverage": "vitest run --coverage",
    "coverage": "vitest run --coverage",
    "coverage:watch": "vitest --coverage",
    "e2e": "playwright test -c playwright.config.ts",
    "e2e:ui": "playwright test --ui -c playwright.config.ts",
    "e2e:debug": "playwright test --debug -c playwright.config.ts",
    "e2e:remote": "BASE_URL=${BASE_URL:-http://localhost:3000} playwright test -c playwright.remote.config.ts",
    "test:a11y:dialog": "playwright test tests/a11y/dialog-focus-trap.spec.ts",
    "test:a11y:isolated": "pnpm -C packages/pw-tests test",
    "test:a11y:isolated:list": "pnpm -C packages/pw-tests test --list",
    "warning:debt": "node scripts/warn-debt.cjs",
    "check:a11y-buttons": "node scripts/check-icon-buttons.mjs",
    "chaos:light": "cross-env CHAOS_ENABLED=true CHAOS_LATENCY_MS=150 CHAOS_FAILURE_RATE=0.05 CHAOS_TIMEOUT_RATE=0.02 CHAOS_SUPABASE_FAIL_RATE=0.10 playwright test tests/chaos",
    "chaos:storm": "cross-env CHAOS_ENABLED=true CHAOS_LATENCY_MS=400 CHAOS_FAILURE_RATE=0.15 CHAOS_TIMEOUT_RATE=0.08 CHAOS_SUPABASE_FAIL_RATE=0.30 CHAOS_AI_FAIL_RATE=0.25 playwright test tests/chaos",
    "chaos:offline": "playwright test tests/chaos/offline.spec.ts",
    "chaos:a11y": "cross-env CHAOS_ENABLED=true CHAOS_LATENCY_MS=200 CHAOS_FAILURE_RATE=0.10 playwright test tests/chaos/a11y-chaos.spec.ts",
    "chaos:contracts": "cross-env CHAOS_ENABLED=true CHAOS_LATENCY_MS=300 CHAOS_FAILURE_RATE=0.20 CHAOS_TIMEOUT_RATE=0.10 CHAOS_SUPABASE_FAIL_RATE=0.25 playwright test tests/chaos/ux-contracts.spec.ts",
    "chaos:taxonomy": "node scripts/chaos-taxonomy.mjs playwright-report",
    "chaos:full": "pnpm chaos:light && pnpm chaos:contracts && pnpm chaos:taxonomy",
    "test:performance": "pnpm dlx @playwright/test test tests/performance/",
    "quality:debt": "node scripts/warning-debt-burn-down.mjs",
    "quality:badges": "node scripts/generate-quality-badges.mjs",
    "quality:digest": "node scripts/weekly-quality-digest.mjs",
    "test:visual": "playwright test -c playwright.visual.config.ts",
    "test:synthetics": "playwright test -c playwright.synthetics.config.ts",
    "test:update-snaps": "playwright test --update-snapshots -c playwright.config.ts",
    "db:reset": "supabase db reset",
    "db:push": "supabase db push",
    "db:pull": "supabase db pull",
    "db:diff": "supabase db diff",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "ai:test": "node scripts/ai-test-loop.mjs",
    "maintenance:calendar": "node scripts/setup-maintenance-calendar.mjs",
    "i18n:check": "node scripts/check-i18n.mjs",
    "visual:snapshots": "node scripts/generate-visual-snapshots.js",
    "prebuild": "npm run i18n:check",
    "digest:weekly": "node scripts/weekly-quality-digest.mjs",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache --cache-location node_modules/.cache/eslint",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.{css,scss}": [
      "prettier --write"
    ],
    "*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@formspree/react": "^3.0.0",
    "@fullcalendar/core": "^6.1.17",
    "@fullcalendar/daygrid": "^6.1.17",
    "@fullcalendar/interaction": "^6.1.17",
    "@fullcalendar/react": "^6.1.17",
    "@fullcalendar/timegrid": "^6.1.17",
    "@hookform/resolvers": "^3.9.1",
    "@popperjs/core": "^2.11.8",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@react-leaflet/core": "^2.1.0",
    "@sentry/nextjs": "^8.3.0",
    "@stripe/react-stripe-js": "^2.6.2",
