{
  "name": "bookiji",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:setup": "pnpm install && pnpm db:start && pnpm dev",
    "dev:kill": "powershell -ExecutionPolicy Bypass -File scripts/kill-dev-server.ps1",
    "dev:cron": "tsx scripts/local-cron-scheduler.ts",
    "env:dev": "dotenv -e .env.development --",
    "env:staging": "dotenv -e .env.staging --",
    "env:prod": "dotenv -e .env.production --",
    "dev:start": "cross-env DOTENV_CONFIG_PATH=.env.dev next dev -p 3000",
    "staging:start": "cross-env DOTENV_CONFIG_PATH=.env.staging next start -p 3000",
    "prod:start": "cross-env DOTENV_CONFIG_PATH=.env.prod next start -p 3000",
    "build": "node scripts/build-without-supabase-functions.js",
    "vercel:build": "node scripts/build-without-supabase-functions.js",
    "start": "next start",
    "start:ci": "next start -p 3000",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "context": "node startup-context.js",
    "dev:diagnose": "tsx scripts/dev-diagnose.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:fast": "pnpm test:run",
    "test:run": "node scripts/vitest-split-run.mjs",
    "test:run:split": "node scripts/vitest-split-run.mjs",
    "test:integration": "vitest run tests/integration",
    "test:coverage": "vitest run --coverage",
    "e2e": "playwright test",
    "e2e:all": "pnpm test:run && pnpm e2e",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "e2e:diagnose": "tsx scripts/e2e/diagnose-env.ts",
    "protect:env": "tsx scripts/protect-env-local.ts",
    "e2e:sync-env": "tsx scripts/e2e/sync-env.ts",
    "e2e:force-remote": "tsx scripts/e2e/force-remote-env.ts",
    "e2e:proof": "playwright test --config=playwright.proof.config.ts",
    "e2e:proof:headed": "playwright test --config=playwright.proof.config.ts --headed",
    "e2e:scheduling-proof": "tsx scripts/e2e/verify-web-server.ts && playwright test --config=playwright.scheduling-proof.config.ts",
    "e2e:seed": "cross-env DOTENV_CONFIG_PATH=.env.e2e tsx -r dotenv/config scripts/e2e/seed-users.ts",
    "e2e:check": "tsx scripts/e2e/check-prerequisites.ts",
    "e2e:setup-remote": "tsx scripts/e2e/setup-remote-supabase.ts",
    "e2e:navigation": "cross-env DOTENV_CONFIG_PATH=.env.prod RUNTIME_MODE=prod tsx scripts/e2e/run-navigation-completeness.ts",
    "db:reset-users": "tsx scripts/reset-users.ts",
    "proof:all": "pnpm db:reset-users && pnpm e2e:seed && pnpm e2e:proof:headed || (echo '‚ùå Proof suite failed - check test-results/proof and playwright-report/proof' && exit 1) && playwright show-report",
    "certify:staging": "pnpm env:staging sh -c 'ALLOW_DESTRUCTIVE_USER_RESET=true ADMIN_EMAIL=${ADMIN_EMAIL:-admin@bookiji.com} pnpm proof:all'",
    "contract": "playwright test --config=playwright.contract.config.ts",
    "loadtest": "k6 run loadtests/booking-flow.k6.js",
    "chaos": "playwright test tests/chaos/",
    "drift": "tsx scripts/check-drift.ts",
    "docs:integrity": "tsx scripts/docCodeIntegrity.ts",
    "docs:index-lint": "tsx scripts/docs-index-lint.ts",
    "db:reset": "tsx scripts/env/supabase-guarded.ts db reset",
    "db:push": "tsx scripts/env/supabase-guarded.ts db push",
    "db:pull": "tsx scripts/env/supabase-guarded.ts db pull",
    "db:diff": "supabase db diff",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:grant-admin": "node scripts/run-grant-admin-local.mjs",
    "db:refresh-metrics": "node scripts/refresh-api-metrics.mjs",
    "supabase:start": "node scripts/supabase-start.mjs",
    "supabase:stop": "node scripts/supabase-stop.mjs",
    "db:policy:check": "node scripts/check-db-policy.mjs",
    "recovery:import:db": "node -e \"if(process.env.RECOVERY_ENV!=='1'){console.error('RECOVERY_ENV=1 is required.');process.exit(1)}\" && node scripts/recovery/_lib/run-import-db.mjs",
    "recovery:import:storage": "node -e \"if(process.env.RECOVERY_ENV!=='1'){console.error('RECOVERY_ENV=1 is required.');process.exit(1)}\" && node scripts/recovery/_lib/run-import-storage.mjs",
    "recovery:verify:db": "node -e \"if(process.env.RECOVERY_ENV!=='1'){console.error('RECOVERY_ENV=1 is required.');process.exit(1)}\" && tsx scripts/recovery/verify_db.ts",
    "recovery:verify:storage": "node -e \"if(process.env.RECOVERY_ENV!=='1'){console.error('RECOVERY_ENV=1 is required.');process.exit(1)}\" && tsx scripts/recovery/verify_storage.ts",
    "recovery:verify": "node -e \"if(process.env.RECOVERY_ENV!=='1'){console.error('RECOVERY_ENV=1 is required.');process.exit(1)}\" && pnpm recovery:verify:db && pnpm recovery:verify:storage",
    "recovery:doctor": "node scripts/recovery/doctor.mjs",
    "ai:test": "node scripts/ai-test-loop.mjs",
    "maintenance:calendar": "node scripts/setup-maintenance-calendar.mjs",
    "i18n:check": "node scripts/i18n-check.js",
    "i18n:translate": "node scripts/translate-missing-keys.mjs",
    "i18n:translate:all": "node scripts/translate-all-keys.mjs",
    "seed:pilot": "tsx scripts/seed-pilot-data.ts",
    "seo:check": "tsx scripts/check-sitemap.ts",
    "seo:test": "concurrently -k -s first \"pnpm start:ci\" \"wait-on http://localhost:3000 && pnpm seo:check\"",
    "seo:indexnow": "tsx scripts/indexnow-push.ts",
    "bootstrap-kb": "tsx scripts/bootstrap-bookiji-kb.ts",
    "healthai": "tsx scripts/healthai-monitor.ts",
    "deployai": "tsx scripts/deployai-monitor.ts",
    "anomalyai": "tsx scripts/anomalyai-monitor.ts",
    "ops:smoke": "tsx scripts/ops-smoke-test.ts",
    "ci:epistemic": "node scripts/ci-epistemic-gates.mjs",
    "check:cursorrules": "node scripts/check-cursorrules-sync.mjs",
    "prebuild": "cross-env ALLOW_INCOMPLETE_I18N=true npm run i18n:check -- --auto-fill",
    "prepare": "husky install",
    "build:opsai-sdk": "pnpm exec tsc -p packages/opsai-sdk/tsconfig.json",
    "build:voice-console": "pnpm --dir packages/opsai-voice run build",
    "build:opsai-helpdesk": "pnpm exec tsc -p packages/opsai-helpdesk/tsconfig.json",
    "build:opsai-l7": "pnpm exec tsc -p packages/opsai-l7/tsconfig.json",
    "dev:controlplane": "pnpm --dir apps/opsai-control-plane dev",
    "build:controlplane": "pnpm --dir apps/opsai-control-plane build",
    "bootstrap": "pnpm run build:opsai-sdk && pnpm run build:opsai-helpdesk && pnpm run build:opsai-l7 && pnpm run build:voice-console && pnpm run build:controlplane",
    "invariants:check": "node scripts/check-invariants-policy.mjs && node scripts/check-jarvis-phase4-invariants.mjs && node scripts/check-jarvis-phase5-invariants.mjs",
    "monday:executive": "node scripts/monday/sync-executive-dashboard.mjs"
  },
  "dependencies": {
    "@faker-js/faker": "^10.0.0",
    "@formspree/react": "^3.0.0",
    "@fullcalendar/core": "^6.1.17",
    "@fullcalendar/daygrid": "^6.1.17",
    "@fullcalendar/interaction": "^6.1.17",
    "@fullcalendar/react": "^6.1.17",
